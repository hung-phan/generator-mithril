{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "dependencies": {<% if (cssFramework === 'SASSBootstrap') { %>
    "sass-bootstrap": "*",<% } %><% if (includeFontAwesome) { %>
    "font-awesome": "*",<% } %><% if (includeLodash) { %>
    "lodash": "*",<% } %><% if (includeModernizr) { %>
    "modernizr": "*",<% } %>
    "mithril": "*",
    "jquery": "~2.1.3",<% if (includeButtonCss) { %>
    "Buttons": "*",<% } %><% if (includeAnimateCss) { %>
    "animate-sass": "*",<% } %><% if (moduleLoader === 'requirejs') { %>
    "requirejs": "*",<% } %>
    "almond": "*"
  }
}

